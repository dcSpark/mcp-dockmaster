{
  "name": "mcp-proxy-server",
  "version": "0.1.0",
  "description": "A proxy server for MCP Protocol commands",
  "private": true,
  "bin": {
    "mcp-proxy-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "dev": "npx ts-node-dev --respawn src/index.ts",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.6.1",
    "lunr": "^2.3.9",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/lunr": "^2.3.7",
    "@types/node": "^22.13.9",
    "@types/node-fetch": "3.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.8.2"
  }
}
